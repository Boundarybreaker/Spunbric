{
  "schemaVersion": 1,
  "id": "spunbric",
  "version": "${version}",

  "name": "Spunbric",
  "description": "An implementation of the SpongeAPI on fabric",
  "authors": [
  ],
  "contact": {
    "homepage": "https://www.spongepowered.org/",
    "sources": "https://github.com/SpongePowered"
  },

  "license": "MIT",
  "icon": "assets/sponge/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "org.spongepowered.mod.client.SpongeClientMod"
    ],
    "server": [
      "org.spongepowered.mod.server.SpongeServerMod"
    ]
  },
  "mixins": [
    "spunbric.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "suggests": {
    "modmenu": "*"
  },

  "custom": {
    "modmenu:api": false
  }
}